<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hxl.ssm.mapper.UserMapper">

    <select id="queryUserById" resultType="com.hxl.ssm.domain.entity.MyUser">
        select * from my_user
        where id = #{userId}
    </select>
</mapper>